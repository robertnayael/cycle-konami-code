{
    "extends": [
        "tslint-config-airbnb"
    ],
    "rules": {
        "import-name": false,
        "ter-indent": [true, 4],
        "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
        "semicolon": [true, "never"],
        "function-name": [true, {"function-regex": "^[a-zA-Z$][\\w\\d]+$"}],
        "array-bracket-spacing": [true, "always"]
    }
}
